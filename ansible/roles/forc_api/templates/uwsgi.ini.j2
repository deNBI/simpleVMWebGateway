module = manage:app
venv = {{ FORC_INSTALLATION_PATH }}simpleVMWebGateway/FlaskOpenRestyConfigurator/venv/
master = true
processes = 5
logto = /var/log/forc.log

socket = /var/run/forc.sock
vacuum = true

chmod-socket = 776
buffer-size = 65535